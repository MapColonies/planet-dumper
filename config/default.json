{
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    },
    "tracing": {
      "enabled": false,
      "url": "http://localhost:4318/v1/trace"
    }
  },
  "httpClient": {
    "timeout": 1000
  },
  "postgres": {
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    },
    "sslMode": "require"
  },
  "pgDump": {
    "verbose": false
  }
}
