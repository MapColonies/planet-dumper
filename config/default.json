{
  "logger": {
    "level": "info",
    "prettyPrint": false
  },
  "httpClient": {
    "timeout": 1000
  },
  "app": {
    "dumpName": {
      "dumpPrefix": {
        "enabled": false
      },
      "addTimestamp": true,
      "value": "dumpName"
    },
    "dumpUpload": {
      "s3": true,
      "dumpServer": true
    }
  },
  "s3": {
    "endpoint": "http://localhost:9000/",
    "bucketName": "planet-dumps",
    "acl": "public-read"
  },
  "postgres": {
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    }
  },
  "pgDump": {
    "verbose": true
  },
  "dumpServer": {
    "endpoint": "https://dump-server.com",
    "token": {
      "enabled": true,
      "value": "dQw4w9WgXcQ"
    }
  }
}
