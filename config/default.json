{
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    },
    "tracing": {
      "enabled": false,
      "url": "http://localhost:4318/v1/trace"
    }
  },
  "httpClient": {
    "timeout": 1000
  },
  "postgres": {
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    }
  },
  "pgDump": {
    "verbose": false
  },
  "arstotzka": {
    "enabled": false,
    "serviceId": "serviceId",
    "mediator": {
      "timeout": 10000,
      "enableRetryStrategy": false,
      "actiony": {
        "url": "http://actiony.com"
      },
      "locky": {
        "url": "http://locky.com"
      }
    }
  }
}
