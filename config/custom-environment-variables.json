{
  "telemetry": {
    "serviceName": "TELEMETRY_SERVICE_NAME",
    "hostname": "TELEMETRY_HOST_NAME",
    "version": "TELEMETRY_SERVICE_VERSION",
    "logger": {
      "level": "LOG_LEVEL",
      "prettyPrint": {
        "__name": "LOG_PRETTY_PRINT_ENABLED",
        "__format": "boolean"
      }
    },
    "tracing": {
      "enabled": {
        "__name": "TELEMETRY_TRACING_ENABLED",
        "__format": "boolean"
      },
      "url": "TELEMETRY_TRACING_URL"
    }
  },
  "httpClient": {
    "timeout": {
      "__name": "HTTP_CLIENT_TIMEOUT",
      "__format": "number"
    }
  },
  "postgres": {
    "enableSslAuth": {
      "__name": "POSTGRES_ENABLE_SSL_AUTH",
      "__format": "boolean"
    },
    "sslPaths": {
      "ca": "POSTGRES_CA_PATH",
      "key": "POSTGRES_KEY_PATH",
      "cert": "POSTGRES_CERT_PATH"
    }
  },
  "pgDump": {
    "verbose": {
      "__name": "PG_DUMP_VERBOSE",
      "__format": "boolean"
    }
  },
  "ngDump": {
    "verbose": {
      "__name": "NG_DUMP_VERBOSE",
      "__format": "boolean"
    },
    "maxConcurrency": {
      "__name": "NG_DUMP_MAX_CONCURRENCY",
      "__format": "number"
    }
  },
  "arstotzka": {
    "enabled": {
      "__name": "ARSTOTZKA_ENABLED",
      "__format": "boolean"
    },
    "services": {
      "__name": "ARSTOTZKA_SERVICES",
      "__format": "json"
    },
    "mediator": {
      "timeout": {
        "__name": "MEDIATOR_TIMEOUT",
        "__format": "number"
      },
      "enableRetryStrategy": {
        "__name": "MEDIATOR_ENABLE_RETRY_STRATEGY",
        "__format": "boolean"
      },
      "retryStrategy": {
        "retries": {
          "__name": "MEDIATOR_RETRY_STRATEGY_RETRIES",
          "__format": "number"
        },
        "shouldResetTimeout": {
          "__name": "MEDIATOR_RETRY_STRATEGY_SHOULD_RESET_TIMEOUT",
          "__format": "boolean"
        },
        "isExponential": {
          "__name": "MEDIATOR_RETRY_STRATEGY_IS_EXPONENTIAL",
          "__format": "boolean"
        },
        "delay": {
          "__name": "MEDIATOR_RETRY_STRATEGY_DELAY",
          "__format": "number"
        }
      },
      "actiony": {
        "url": "MEDIATOR_ACTIONY_URL"
      },
      "locky": {
        "url": "MEDIATOR_LOCKY_URL"
      }
    }
  }
}
